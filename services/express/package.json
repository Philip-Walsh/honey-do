{
  "name": "philip.dev.express",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/src/server.js",
    "start:dev": "nodemon src/server.ts",
    "test:integration": "jest --testMatch='**/integration/**/*.test.ts' --detectOpenHandles --no-cache",
    "test:integration:now": "jest --testMatch='**/integration/**/menu/**/*.test.ts' --detectOpenHandles --no-cache --runInBand",
    "test:unit": "jest --testMatch='**/unit/**/*.test.ts'",
    "test:watch": "jest --testMatch='/*.test.ts' --detectOpenHandles --watchAll --no-cache",
    "test": "npm run test:unit && npm run test:integration",
    "lint": "prettier **/*.ts --check && eslint **/*.ts",
    "lint:fix": "prettier . --write && eslint . --fix",
    "seed": "ts-node-dev src/scripts/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Philip-Walsh/personal_site.git"
  },
  "author": "Philip Walsh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Philip-Walsh/personal_site/issues"
  },
  "homepage": "https://github.com/Philip-Walsh/personal_site#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb": "^6.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "nyc": {
    "report-dir": "coverage",
    "reporter": [
      "lcov"
    ],
    "temp-directory": "coverage"
  }
}
